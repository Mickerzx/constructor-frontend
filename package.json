{
  "name": "constructor-frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc:check": "npx tsc",
    "stylelint:check": "stylelint src/**/*.{css,scss,sass}",
